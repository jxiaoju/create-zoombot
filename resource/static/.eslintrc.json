{

  "parser": "babel-eslint",
  "env": {
    "node": true,
    "browser": true,
    "es6": true
  },
  "globals": {
    "Office": true
  },
  "parserOptions": {
    "ecmaVersion": 9,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "quotes": [
      "error",
      "single"
    ],
    "react/jsx-uses-react": "warn",
    "react/jsx-uses-vars": "warn",
    "no-console": "off",
    "no-extra-semi": [
      "off"
    ],
    "semi": [
      "warn",
      "always"
    ],
    "no-unused-vars": [
      "warn"
    ],
    "no-unused-expressions": [
      0,
      {
        "allowTernary": true,
        "allowShortCircuit": true,
        "allowTaggedTemplates": true
      }
    ]
  }
}